{
    "name": "@tractorzoom/dynamo-utils",
    "version": "2.3.0",
    "description": "Wrapper around the JS Dynamo Doc Client implementing standardized error handling and other utilities",
    "main": "lib/index.js",
    "author": "@TractorZoom",
    "license": "MIT",
    "engines": {
        "node": "12.x"
    },
    "scripts": {
        "build": "./node_modules/.bin/babel src --out-dir lib --copy-files",
        "commit": "git-cz",
        "prettier:all": "prettier --write './**/*.*'",
        "pretty-quick": "pretty-quick",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/cli": "7.12.13",
        "@babel/core": "7.12.13",
        "@babel/preset-env": "7.12.13",
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/release-notes-generator": "9.0.1",
        "@tractorzoom/chance-the-wrapper": "2.1.68",
        "commitizen": "4.2.3",
        "husky": "4.3.8",
        "jest": "26.6.3",
        "prettier": "2.2.1",
        "pretty-quick": "2.0.2",
        "semantic-release": "17.3.8"
    },
    "dependencies": {
        "aws-sdk": "2.839.0"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/TractorZoom/dynamo-utils.git"
    },
    "homepage": "https://github.com/TractorZoom/dynamo-utils",
    "bugs": {
        "url": "https://github.com/TractorZoom/dynamo-utils/issues"
    },
    "keywords": [
        "aws",
        "aws-dynamo",
        "database",
        "dynamo",
        "dynamodb"
    ]
}
